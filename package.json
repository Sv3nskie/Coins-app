{
  "name": "ts-nextjs-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build -p 80",
    "start": "next start -p 80",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "tsc": "tsc --noEmit",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "postbuild": "next-sitemap",
    "prepare": "husky install"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "^1.7.1",
    "@algolia/autocomplete-theme-classic": "^1.7.1",
    "@headlessui/react": "^1.4.2",
    "@preline/overlay": "^1.2.0",
    "clsx": "^1.1.1",
    "kaktana-react-lightweight-charts": "^2.0.0",
    "lightweight-charts": "^3.7.0",
    "next": "^12.0.7",
    "next-themes": "^0.0.15",
    "next-transpile-modules": "^9.1.0",
    "preline": "^1.2.1",
    "react": "^17.0.2",
    "react-charts": "^3.0.0-beta.34",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-responsive": "^9.0.0-beta.5",
    "react-tradingview-embed": "^3.0.6",
    "react-tradingview-technical-analysis": "^1.0.2",
    "react-tradingview-widget": "^1.3.2",
    "react-ts-tradingview-widgets": "^1.0.11",
    "swiper": "^7.4.1",
    "tailwind-merge": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@svgr/webpack": "^6.1.2",
    "@tailwindcss/forms": "^0.4.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.38",
    "@types/tailwindcss": "^2.2.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.5",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.5",
    "lint-staged": "^11.2.6",
    "next-sitemap": "^1.6.203",
    "postcss": "^8.4.17",
    "postcss-import": "^15.0.0",
    "postcss-nested": "^6.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-sort-class-names": "^3.0.1",
    "react-test-renderer": "^17.0.2",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  }
}
